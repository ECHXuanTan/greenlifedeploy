 <Nav.Item>\r\n              <strong>DANH MỤC SẢN PHẨM</strong>\r\n            </Nav.Item>\r\n            {categories.map((category) => (\r\n              <Nav.Item key={category}>\r\n                <LinkContainer\r\n                  to={{\r\n                    pathname: \"/search\",\r\n                    search: `?category=${category}`,\r\n                  }}\r\n                  onClick={() => setSidebarIsOpen(false)}\r\n                >\r\n                  <Nav.Link>{category}</Nav.Link>\r\n              </LinkContainer>\r\n              </Nav.Item>\r\n            ))}\r\n          </Nav>\r\n        </div>\r\n        <main>\r\n          <Container>\r\n            <Routes>\r\n            <Route\r\n                path=\"/admin/products\"\r\n                element={\r\n                  <AdminRoute>\r\n                    <ProductListScreen />\r\n                  </AdminRoute>\r\n                }\r\n              ></Route>\r\n              <Route path=\"/product/:slug\" element={<ProductScreen />} />\r\n              <Route path=\"/cart\" element={<CartScreen />} />\r\n              <Route path=\"/\" element={<HomeScreen />} />\r\n              <Route path=\"/signin\" element={<SigninScreen />} />\r\n              <Route path=\"/signup\" element={<SignupScreen />} />\r\n              <Route\r\n                path=\"/profile\"\r\n                element={\r\n                  <ProtectedRoute>\r\n                    <ProfileScreen />\r\n                  </ProtectedRoute>\r\n                }\r\n              />\r\n              <Route\r\n                path=\"/shipping\"\r\n                element={<ShippingAddressScreen />}\r\n              ></Route>\r\n              <Route path=\"/placeorder\" element={<PlaceOrderScreen />} />\r\n              <Route\r\n                path=\"/order/:id\"\r\n                element={\r\n                  <ProtectedRoute>\r\n                    <OrderScreen />\r\n                  </ProtectedRoute>\r\n                }\r\n              ></Route>\r\n              <Route\r\n                path=\"/orderhistory\"\r\n                element={\r\n                  <ProtectedRoute>\r\n                    <OrderHistoryScreen />\r\n                  </ProtectedRoute>\r\n                }\r\n              ></Route>\r\n              <Route path=\"/search\" element={<SearchScreen />} />\r\n              {/* Admin Routes */}\r\n              <Route\r\n                path=\"/admin/dashboard\"\r\n                element={\r\n                  <AdminRoute>\r\n                    <DashboardScreen />\r\n                  </AdminRoute>\r\n                }\r\n              ></Route>\r\n              <Route\r\n                path=\"/admin/product/:id\"\r\n                element={\r\n                  <AdminRoute>\r\n                    <ProductEditScreen />\r\n                  </AdminRoute>\r\n                }\r\n              ></Route>\r\n              <Route\r\n                path=\"/admin/orders\"\r\n                element={\r\n                  <AdminRoute>\r\n                    <OrderListScreen />\r\n                  </AdminRoute>\r\n                }\r\n              ></Route>\r\n            </Routes>\r\n          </Container>\r\n        </main>\r\n        <footer>\r\n          <div className=\"text-center\">@GreenLife</div>\r\n        </footer>\r\n      </div>\r\n    </BrowserRouter>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","D:\\TMDT\\greenlife-deploy\\fe\\src\\store.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\utils.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\HomeScreen.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\ProductScreen.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\ShippingAddressScreen.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\CartScreen.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\OrderHistoryScreen.js",[],[],"D:\\TMDT\\greenlife-deploy\\fe\\src\\screens\\PlaceOrderScreen.js",[],[],"D:\\TMDT\\greenlife-de